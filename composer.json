{
    "name": "neverbounce/neverbounce-laravel",
    "description": "This provides a laravel service provider for the NeverBounceAPI PHP bindings",
    "keywords": ["laravel", "email", "validation", "verification", "email validation"],
    "homepage": "https://neverbounce.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Mollick",
            "email": "mike@creavos.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
        "neverbounce/neverbounce-php": "1.1.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "NeverBounce\\NeverBounceLaravel\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
